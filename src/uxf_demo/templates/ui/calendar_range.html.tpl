<section id="calendar_range">
    <h1 class="line">Calendar Range</h1>
    <div class="section-contents">
        <div class="box box-white">
            <div class="calendar-range" data-start_name="Departure" data-end_name="Return"></div>
        </div>

<pre class="code language-markup">
&lt;div class=&quot;calendar-range&quot; data-start_name=&quot;Departure&quot; data-end_name=&quot;Return&quot;&gt;&lt;/div&gt;
</pre>

        <table class="table-data">
            <tr>
                <td><span class="highlight">data-start</span></td>
                <td><span class="highlight">Date</span></td>
                <td>The initial starting date.</td>
            </tr>
            <tr>
                <td><span class="highlight">data-end</span></td>
                <td><span class="highlight">Date</span></td>
                <td>The initial ending date.</td>
            </tr>
            <tr>
                <td><span class="highlight">data-start_name</span></td>
                <td><span class="highlight">string</span></td>
                <td>The name of the starting date input.</td>
            </tr>
            <tr>
                <td><span class="highlight">data-end_name</span></td>
                <td><span class="highlight">string</span></td>
                <td>The name of the ending date input.</td>
            </tr>
            <tr>
                <td><span class="highlight">data-format</span></td>
                <td><span class="highlight">string</span></td>
                <td>The data format. It's<span class="highlight">%y/%m/%d</span> by default.</td>
            </tr>
        </table>

    </div>
</section>
